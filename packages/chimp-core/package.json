{
  "name": "@chimp-stack/core",
  "version": "0.1.0",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./cli/addChimpConfigCommand": {
      "import": "./dist/cli/addChimpConfigCommand.js",
      "require": "./dist/cli/addChimpConfigCommand.js"
    },
    "./cli/config": {
      "import": "./dist/cli/config.js",
      "require": "./dist/cli/config.js"
    },
    "./cli/init": {
      "import": "./dist/cli/init.js",
      "require": "./dist/cli/init.js"
    },
    "./config": {
      "import": "./dist/config.js",
      "require": "./dist/config.js"
    },
    "./env": {
      "import": "./dist/env.js",
      "require": "./dist/env.js"
    },
    "./types/config": {
      "import": "./dist/types/config.js",
      "require": "./dist/types/config.js"
    },
    "./utils/changelog": {
      "import": "./dist/utils/changelog.js",
      "require": "./dist/utils/changelog.js"
    },
    "./utils/logger": {
      "import": "./dist/utils/logger.js",
      "require": "./dist/utils/logger.js"
    },
    "./utils/openai": {
      "import": "./dist/utils/openai.js",
      "require": "./dist/utils/openai.js"
    }
  },
  "paths": {
    "@chimp-stack/core": ["src/index.ts"],
    "@chimp-stack/core/*": ["src/*"]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Chimp-Stack/chimp-stack.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --out-dir dist"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "tsup": "^8.5.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.3",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "inquirer": "^12.6.3"
  }
}
